<resources xmlns:android="http://schemas.android.com/apk/res/android">
<style name="Scroller">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/slider_back</item>
        <item name="android:layout_margin">0dp</item>
    </style>
     <style name="CustomProgressBar" parent="android:Widget.ProgressBar.Horizontal">
          <item name="android:indeterminateOnly">false</item>
          <item name="android:progressDrawable">@drawable/custom_progressbar</item>
          <item name="android:minHeight">4dip</item>
          <item name="android:maxHeight">12dip</item>
        </style> 
        <style name="CustomProfileProgressBar" parent="android:Widget.ProgressBar.Horizontal">
          <item name="android:indeterminateOnly">false</item>
          <item name="android:progressDrawable">@drawable/profile_perc_bar</item>
          <item name="android:minHeight">12dip</item>
          <item name="android:maxHeight">12dip</item>
        </style> 
       
    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppTheme" parent="Theme.Sherlock.Light.DarkActionBar">
        <item name="android:homeAsUpIndicator">@drawable/new_indicator</item>
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    
    <style name="prefcat" >
        <item name="android:background">@android:color/darker_gray</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="styleName" parent="@style/Theme.Sherlock.Light.NoActionBar">
        <item name="android:focusable">false</item>
    </style>
</resources>